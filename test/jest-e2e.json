{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "../",
  "testRegex": ".*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^types$": "<rootDir>/types",
    "^utils/(.*)$": "<rootDir>/utils/$1",
    "^config$": "<rootDir>/config"
  },
  "setupFilesAfterEnv": ["<rootDir>/test/jest.setup.ts"],
  "testEnvironment": "node",
  "testTimeout": 90000
}
